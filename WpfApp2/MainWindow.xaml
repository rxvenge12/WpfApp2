<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="User Events Example" Height="350" Width="525">
    <Grid>
        <StackPanel Margin="20">
            <Button x:Name="moveButton" Content="Move User 1" Click="MoveButton_Click" Margin="0 0 0 10"/>
            <Button x:Name="compressButton" Content="Compress User 2" Click="CompressButton_Click" Margin="0 0 0 10"/>
            <TextBox x:Name="outputTextBox" IsReadOnly="True" VerticalScrollBarVisibility="Auto" Height="200"/>
        </StackPanel>
    </Grid>
</Window>
